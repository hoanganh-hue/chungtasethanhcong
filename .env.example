# Environment Variables for OpenManus-Youtu Integrated Framework
# Copy this file to .env and fill in your actual values

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional: Backup Supabase instance
SUPABASE_BACKUP_URL=https://your-backup-project.supabase.co
SUPABASE_BACKUP_KEY=your_backup_anon_key_here

# =============================================================================
# TELEGRAM BOT CONFIGURATION
# =============================================================================
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_BOT_USERNAME=your_bot_username
TELEGRAM_WEBHOOK_URL=https://your-domain.com/webhook/telegram
TELEGRAM_WEBHOOK_SECRET=your_webhook_secret_here

# =============================================================================
# API CONFIGURATION
# =============================================================================
API_BASE_URL=http://localhost:8000
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here

# =============================================================================
# DATABASE CONFIGURATION (Alternative to Supabase)
# =============================================================================
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=openmanus_youtu
POSTGRES_USER=openmanus
POSTGRES_PASSWORD=your_postgres_password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
JWT_SECRET_KEY=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
ENVIRONMENT=development
LOG_LEVEL=info
DEBUG=true
API_HOST=0.0.0.0
API_PORT=8000

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================
MAX_WORKERS=4
WORKER_TIMEOUT=30
KEEP_ALIVE=2

# =============================================================================
# MONITORING CONFIGURATION
# =============================================================================
PROMETHEUS_ENABLED=true
METRICS_PORT=9090

# =============================================================================
# RATE LIMITING CONFIGURATION
# =============================================================================
RATE_LIMIT_REQUESTS=1000
RATE_LIMIT_WINDOW=3600
RATE_LIMIT_BURST=100

# =============================================================================
# BACKUP CONFIGURATION
# =============================================================================
BACKUP_BUCKET=your-backup-bucket
BACKUP_REGION=us-west-2

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================
# Test database (optional)
SUPABASE_TEST_URL=https://your-test-project.supabase.co
SUPABASE_TEST_KEY=your_test_anon_key_here

# Mock data generation
GENERATE_MOCK_DATA=false
MOCK_DATA_SIZE=1000

# =============================================================================
# MODULE-SPECIFIC CONFIGURATION
# =============================================================================

# CCCD Generation
CCCD_MAX_QUANTITY=1000
CCCD_ALLOWED_PROVINCES=Hà Nội,TP. Hồ Chí Minh,Hưng Yên,Hải Phòng,Đà Nẵng

# CCCD Check
CCCD_CHECK_TIMEOUT=30
CCCD_CHECK_RETRY_ATTEMPTS=3

# Tax Lookup
TAX_LOOKUP_API_ENDPOINT=https://masothue.com/tra-cuu-ma-so-thue-ca-nhan/
TAX_LOOKUP_RATE_LIMIT=10

# Web Scraping
WEB_SCRAPING_MAX_PAGES=100
WEB_SCRAPING_TIMEOUT=180
WEB_SCRAPING_DELAY=1

# Data Analysis
DATA_ANALYSIS_MAX_SIZE=10485760
DATA_ANALYSIS_TIMEOUT=300

# Form Automation
FORM_AUTOMATION_MAX_FORMS=10
FORM_AUTOMATION_TIMEOUT=120

# Report Generation
REPORT_GENERATION_MAX_REPORTS=5
REPORT_GENERATION_TIMEOUT=300

# Excel Export
EXCEL_EXPORT_MAX_EXPORTS=10
EXCEL_EXPORT_TIMEOUT=180
EXCEL_EXPORT_MAX_ROWS=100000

# =============================================================================
# NOTIFICATION CONFIGURATION
# =============================================================================
# Email notifications (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_email_password

# Slack notifications (optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/slack/webhook

# Discord notifications (optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your/discord/webhook

# =============================================================================
# CLOUD STORAGE CONFIGURATION (Optional)
# =============================================================================
# AWS S3
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2
AWS_S3_BUCKET=your-s3-bucket

# Google Cloud Storage
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_CLOUD_CREDENTIALS_PATH=path/to/credentials.json

# Azure Blob Storage
AZURE_STORAGE_ACCOUNT_NAME=your_storage_account
AZURE_STORAGE_ACCOUNT_KEY=your_storage_key
AZURE_STORAGE_CONTAINER_NAME=your_container

# =============================================================================
# EXTERNAL API CONFIGURATION
# =============================================================================
# OpenAI API (for AI features)
OPENAI_API_KEY=your_openai_api_key

# Google API (for search and other services)
GOOGLE_API_KEY=your_google_api_key

# Facebook API (for social media integration)
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================
LOG_FILE_PATH=logs/app.log
LOG_MAX_SIZE=10485760
LOG_BACKUP_COUNT=5
LOG_FORMAT=json

# =============================================================================
# CACHING CONFIGURATION
# =============================================================================
CACHE_TTL=300
CACHE_MAX_SIZE=1000
CACHE_BACKEND=redis

# =============================================================================
# SESSION CONFIGURATION
# =============================================================================
SESSION_SECRET_KEY=your_session_secret_key
SESSION_TIMEOUT=3600
SESSION_COOKIE_SECURE=true
SESSION_COOKIE_HTTPONLY=true